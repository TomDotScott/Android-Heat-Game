<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GameOver"
    tools:layout_editor_absoluteX="-24dp"
    tools:layout_editor_absoluteY="-39dp">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/panel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="327dp"
        android:layout_height="119dp"
        android:fontFamily="@font/squares"
        android:text="Game Over"
        android:textAlignment="center"
        android:textColor="#607AD6"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.229" />

    <ImageButton
        android:id="@+id/game_over_main_menu_button"
        android:layout_width="147dp"
        android:layout_height="70dp"
        android:layout_marginStart="144dp"
        android:adjustViewBounds="true"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitStart"
        android:src="@drawable/button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="147dp"
        android:layout_height="57dp"
        android:fontFamily="@font/squares"
        android:text="Main Menu"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="6sp"
        app:layout_constraintBottom_toBottomOf="@+id/game_over_main_menu_button"
        app:layout_constraintEnd_toEndOf="@+id/game_over_main_menu_button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/game_over_main_menu_button"
        app:layout_constraintTop_toTopOf="@+id/game_over_main_menu_button"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/game_over_restart_button"
        android:layout_width="147dp"
        android:layout_height="70dp"
        android:adjustViewBounds="true"
        android:scaleType="fitStart"
        android:src="@drawable/button"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.95"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/game_over_exit_button"
        android:layout_width="147dp"
        android:layout_height="70dp"
        android:layout_marginEnd="144dp"
        android:adjustViewBounds="true"
        android:scaleType="fitStart"
        android:src="@drawable/button"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.953"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="147dp"
        android:layout_height="32dp"
        android:fontFamily="@font/squares"
        android:text="Restart"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="6sp"
        app:layout_constraintBottom_toBottomOf="@+id/game_over_restart_button"
        app:layout_constraintEnd_toEndOf="@+id/game_over_restart_button"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="@+id/game_over_restart_button"
        app:layout_constraintTop_toTopOf="@+id/game_over_restart_button"
        app:layout_constraintVertical_bias="0.275" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="131dp"
        android:layout_height="31dp"
        android:fontFamily="@font/squares"
        android:text="Exit"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="6sp"
        app:layout_constraintBottom_toBottomOf="@+id/game_over_exit_button"
        app:layout_constraintEnd_toEndOf="@+id/game_over_exit_button"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/game_over_exit_button"
        app:layout_constraintTop_toTopOf="@+id/game_over_exit_button"
        app:layout_constraintVertical_bias="0.307" />

    <View
        android:id="@+id/stats_bounds"
        android:layout_width="491dp"
        android:layout_height="186dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.554" />

    <TextView
        android:id="@+id/total_trips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="92dp"
        android:text="DELIVERY TEXT"
        android:textAlignment="viewStart"
        app:layout_constraintStart_toStartOf="@+id/stats_bounds"
        app:layout_constraintTop_toTopOf="@+id/stats_bounds" />

    <TextView
        android:id="@+id/average_rating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AVERAGE RATING"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/stats_bounds"
        app:layout_constraintEnd_toEndOf="@+id/stats_bounds"
        app:layout_constraintStart_toStartOf="@+id/stats_bounds"
        app:layout_constraintTop_toTopOf="@+id/stats_bounds"
        app:layout_constraintVertical_bias="0.55" />

    <TextView
        android:id="@+id/total_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:text="TOTAL TIME"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/stats_bounds"
        app:layout_constraintEnd_toEndOf="@+id/stats_bounds"
        app:layout_constraintTop_toTopOf="@+id/stats_bounds"
        app:layout_constraintVertical_bias="0.55" />

</androidx.constraintlayout.widget.ConstraintLayout>